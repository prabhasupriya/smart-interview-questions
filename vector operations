#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin >> n;
    vector<int> arr;
    arr.push_back(10);
    arr.push_back(20);
    arr.push_back(30);
    arr.push_back(40);
    for(int i=0;i<arr.size();i++){
        cout << arr[i];
        cout << endl;
    }
    cout<<"size of the array " << arr.size() <<endl;
    
    arr.pop_back();
    cout <<"array size becomes after removal of the elment "<< arr.size()<<endl;
   cout <<"back  element "<< arr.back()<< endl;
     cout <<"front element "<<arr.front()<<endl;
if(arr.empty()){
    cout <<" vector array is empty"<< endl;
}
else{
    cout << "array contains some elements"<<endl;
}
cout << "sze of array " <<arr.size()<< endl;

arr.insert(arr.begin()+1 , 15);
for(auto i=arr.begin() ; i!= arr.end(); i++){
    cout << *i<< " "<<endl; //i = iterator vector<int>::iterator itera ;
}
arr.insert(arr.begin()+3 , 2, 25);
arr.erase(arr.begin());
for(vector<int>::iterator i = arr.begin() ;i != arr.end() ;i++){
    cout << *i << " ";
    //iteartor pointing to the address or memory of the data of vector 
}
    return 0;
}
