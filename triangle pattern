#include <bits/stdc++.h>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int number;
    cin >> number;
    for(int i=0;i<number;i++){
        for(int j=0;j<=i; j++){
cout<<"*" <<" ";
        }
        cout<<endl;
    }
    return 0;
}
