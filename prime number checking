#include <bits/stdc++.h>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n;
    cin >> n;
    bool found = true;
    if(n ==0 || n ==1){
        cout <<"No";
        return 0;
    }
    for(int i=2;i<=sqrt(n);i++){
        if(n%i ==0){
            found = false;
            break;
        }
    }
    if(found){
        cout <<"Yes";
    }   
    else{
        cout <<"No";
    }
    return 0;
}
