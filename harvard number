#include <bits/stdc++.h>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
   int n;
   cin >> n;
   int temp=n;
   
   int s=0;
   while(temp >0){
    int digit =temp%10;
    s += digit;
    temp /= 10;
   }
   if(n%s == 0){
    cout<<"Yes";
   }
   else{
    cout<<"No";
   }
    return 0;
}
