#include <bits/stdc++.h>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    string a;
    cin >> a;
    for(int i=1;i<a.size();i=i+2){
        
            cout << a[i];
        
    }
    for(int i=0;i<a.size();i=i+2){
cout<<a[i];
    }

    return 0;
}
