#include <bits/stdc++.h>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a,b;
    cin >> a>>b;
    for(int i=0;i<b;i++){
        for(int j=0;j<a;j++){
            if(i==0 || i==b-1 || j==0 || j==a-1){
                cout<<"*";
            }
            else{
                cout<< " ";
            }
        }
        cout << endl;
    }
    return 0;
}
